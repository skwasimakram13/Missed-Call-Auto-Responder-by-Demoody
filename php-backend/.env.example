# Rate Limiting
RATE_LIMIT_PER_DEVICE=100
RATE_LIMIT_PER_PHONE=5

# Business Configuration
DEFAULT_DELAY_MINUTES=5
MAX_RETRY_ATTEMPTS=3
BUSINESS_HOURS_START=09:00
BUSINESS_HOURS_END=18:00